<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="time_sheet_notes" model="ir.ui.view">
        <field name="name">timesheet.wizard.form</field>
        <field name="model">notes.views</field>
        <field name="arch" type="xml">
            <form string="Create Appointment">
                <sheet>
                    <center>
                         <h2 class="text-success">
                            <strong><field name="message_task" readonly="1"/></strong>
                        </h2>
                    </center>
                </sheet>
                <footer>
                    <button string="Yes" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>